
public class Document {
private String name;
private String authorName;
private String publishDate;
private String type;
public Document(String name, String autName, String date,String type){
	this.name = name;
	authorName = autName;
	publishDate = date;
	this.type = type;
}

public String getName(){
	return name;
}

public String getAutName(){
	return authorName;
}

public String getDate(){
	return publishDate;
}
}

