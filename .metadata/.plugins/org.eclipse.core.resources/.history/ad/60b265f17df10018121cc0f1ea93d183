
public class Document {
private String name;
private String authorName;
private String publishDate;
private int price;
public Document(String name, String autName, String date){
	this.name = name;
	authorName = autName;
	publishDate = date;
	
}

public String getName(){
	return name;
}

public String getAutName(){
	return authorName;
}

public String getDate(){
	return publishDate;
}

public int getPrice(){
	return price;
}
}


