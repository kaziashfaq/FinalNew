import java.util.Vector;

public class Buyer implements Observer {
	private Vector<Document> docs;
	Subject subject;
	private String name;
	
	public Buyer(Subject sub){
		subject = sub;
	}
	@Override
	public void update(Vector<Document> docs) {
		// TODO Auto-generated method stub
		this.docs = docs;
		
	}
	
	

}
